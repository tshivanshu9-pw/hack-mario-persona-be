    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.3.1",
    "@nestjs/mongoose": "9.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.3.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/websockets": "^8.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "elastic-apm-node": "^3.37.0",
